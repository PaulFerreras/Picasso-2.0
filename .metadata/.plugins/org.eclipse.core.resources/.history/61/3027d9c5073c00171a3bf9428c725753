package picasso2;

import javax.swing.JToolBar;

public class PicassoToolBarUI extends PicassoAbstractToolBar {

	public PicassoToolBarUI(String name) {
		//PF: Name gets delegated to PicassoAbstractToolBar constructor
		super(name);
		
		//PF: Set orientation to vertical
		setOrientation(JToolBar.VERTICAL);
		
		setPreferredSize(200);
	}

}
