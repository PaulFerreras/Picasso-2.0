package picasso2;

import java.awt.LayoutManager;

import javax.swing.JPanel;

/*PF: This is the container that holds the canvas UI
 * It is resizable
 */

public class ResizableCanvas extends JPanel {

	public ResizableCanvas(LayoutManager lm) {
		super(lm);
		
		/*PF: Create JPanels for 
		 * different resize cursors
		 */
		JPanel south_resize = new JPanel();
		JPanel southeast_resize = new JPanel();
		JPanel east_resize = new JPanel();
		
		
	}
	
}
